{
  "tests": [ "+" ],
  "suiteConf": {
    "port": 3001,
    "extensions": [ "asyncShare" ],
    "generation": {
      "file": "config/bits/generation.js",
      "function": "generateArithmeticInputs"
    },
    "computation": {
      "file": "config/bits/computations.js",
      "function": "compute"
    }
  },

  "testConf": {
    "default": {
      "share": "share_bits",
      "open": "open_bits",
      "count": 1,
      "parallel": 3,
      "options": {
        "Zp": 127,
        "max": 511,
        "party_count": 3
      }
    },
    "*": {
      "share": "share_bits",
      "open": "open_bits",
      "count": 1,
      "parallel": 3,
      "options": {
        "Zp": 127,
        "max": 10,
        "party_count": 3
      }
    },
    "/": {
      "share": "share_bits",
      "open": "composition",
      "count": 1,
      "parallel": 3,
      "options": {
        "Zp": 127,
        "party_count": 3
      }
    },
    "%": {
      "share": "decomposition",
      "open": "composition",
      "count": 1,
      "parallel": 3,
      "options": {
        "Zp": 127,
        "party_count": 3
      }
    }
  }
}